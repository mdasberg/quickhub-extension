{
  "manifest_version": 3,
  "name": "QuickHub",
  "version": "1.0.0",
  "description": "Organize your links in beautiful, customizable groups on your new tab page",
  "permissions": ["storage", "search"],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
